      <Button fx:id="buttonOnline" layoutX="240.0" layoutY="240.0" mnemonicParsing="false" onMouseClicked="#clickedOnline" onMouseEntered="#enteredOnline" onMouseExited="#exitedOnline" prefHeight="40.0" prefWidth="120.0" text="new game online" textAlignment="CENTER" />
      <Text layoutX="175.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="well come to ehsan chess" textAlignment="CENTER" wrappingWidth="250.0">
